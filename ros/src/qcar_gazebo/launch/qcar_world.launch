<launch>	  
	<include file="$(find gazebo_ros)/launch/empty_world.launch">	    
		<arg name="world_name" value="$(find qcar_gazebo)/worlds/qcar.world"/>	    
		<arg name="gui" value="true"/>	
		<arg name="verbose" value="true" />  
	</include>	

	<param name="robot_description" command="$(find xacro)/xacro $(find qcar)/urdf/qcar_model.xacro" />
	<node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-urdf -param robot_description -model qcar" />
	  <!-- send fake joint values -->
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
  </node>


  <!-- Combine joint values -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>

	
	<node name="rviz" pkg="rviz" type="rviz" />
</launch>
